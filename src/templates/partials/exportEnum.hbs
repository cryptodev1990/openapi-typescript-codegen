{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export enum {{{name}}} {
    {{#each enum}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{#if (containsSpaces name)}}
    "{{{name}}}" = {{{value}}},
    {{else}}
    {{{name}}} = {{{value}}},
    {{/if}}
    {{/each}}
}
