{
{{#if link}}
    dictionaryOf: {{>schema link}},
{{else}}
    dictionaryOf: {
        type: '{{{base}}}',
    },
{{/if}}
{{#if isReadOnly}}
    isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
    isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
    isNullable: {{{isNullable}}},
{{/if}}
}
