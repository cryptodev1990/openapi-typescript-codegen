{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export let {{{name}}};
(function ({{{name}}}) {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{{../name}}}.{{{name}}} = {
        {{#each enum}}
        {{{name}}}: {{{value}}}{{>comma}}
        {{/each}}
    };

    {{/each}}
})({{{name}}} || ({{{name}}} = {}));
