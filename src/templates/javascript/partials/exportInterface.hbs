{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export let {{{name}}};
(function ({{{name}}}) {

    {{#if enums}}
    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{{../name}}}.{{{name}}} = {
        {{#each enum}}
        {{{name}}}: {{{value}}},
        {{/each}}
    };

    {{/each}}
    {{/if}}
    {{{name}}}.definition = {{>definition}};

})({{{name}}} || ({{{name}}} = {}));
