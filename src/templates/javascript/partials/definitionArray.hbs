{
    type: 'Array',
{{#if link~}}
    item: {{>definition link}},
{{else}}
    item: {
        type: '{{{base}}}',
    },
{{/if}}
{{#if isReadOnly~}}
    isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired~}}
    isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable~}}
    isNullable: {{{isNullable}}},
{{/if}}
}
