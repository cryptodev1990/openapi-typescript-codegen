{
    properties: {
{{#if extends}}
    {{#each extends}}
        ...{{{this}}}.definition.properties,
    {{/each}}
{{/if}}
{{#if properties}}
    {{#each properties}}
        {{{name}}}: {{>definition}},
    {{/each}}
{{/if}}
    },
{{#if isReadOnly~}}
    isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired~}}
    isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable~}}
    isNullable: {{{isNullable}}},
{{/if}}
}
