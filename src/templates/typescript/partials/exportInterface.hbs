{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export interface {{{name}}}{{>extends}} {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
    {{/each}}
}

export namespace {{{name}}} {

    {{#if enums}}
    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}
    {{/if}}
    export const schema: Schema<{{{name}}}> = {{>schema}};

}
